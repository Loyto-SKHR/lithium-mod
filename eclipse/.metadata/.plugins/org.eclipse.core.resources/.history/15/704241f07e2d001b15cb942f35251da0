package fr.lithium.lithium.common;

import net.minecraft.client.renderer.texture.IIconRegister;
import net.minecraft.item.Item;

public class ItemLithiumSword extends Item {
	
	public ItemLithiumSword() {
		this.setUnlocalizedName("itemTestSword");
	}
	

	@Override
    public void registerIcons(IIconRegister iconregister)
    {
        this.itemIcon = iconregister.registerIcon(ModLithium.MODID + ":lithium-sword");
    }
}
